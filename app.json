{
  "expo": {
    "name": "Peach",
    "slug": "peach",
    "scheme": "peach-app",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.peachapp.dashboard",
      "buildNumber": "1.0.3",
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Peach to access your camera roll to upload an avatar image‚Äù
      }
    }
  }
}
